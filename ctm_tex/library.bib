@article{Venzke2017,
abstract = {—High penetration of renewable energy sources and the increasing share of stochastic loads require the explicit repre-sentation of uncertainty in tools such as the optimal power flow (OPF). Current approaches follow either a linearized approach or an iterative approximation of non-linearities. This paper proposes a convex reformulation of a chance-constrained AC-OPF. Using an affine policy, we can ensure tractability, accurately model large power deviations, and determine suitable control policies for active power, reactive power, and voltage. We consider two types of uncertainty sets. For a rectangular uncertainty set it is sufficient to enforce the chance constraints at its vertices. For a multivariate Gaussian distribution of forecast errors, we propose a novel analytical reformulation of the linear chance constraints and a tractable approximation of the semi-definite chance constraints. We demonstrate the performance of our methods on an IEEE 9 bus system with two wind farms. Our approach satisfies all AC power flow constraints, and is exact, achieving zero relaxation gap.},
archivePrefix = {arXiv},
arxivId = {arXiv:1702.08372v1},
author = {Venzke, Andreas and Halilbasic, Lejla and Markovic, Uros and Hug, Gabriela and Chatzivasileiadis, Spyros},
eprint = {arXiv:1702.08372v1},
file = {::},
