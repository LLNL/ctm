@phdthesis{Castillo2016,
abstract = {The ACOPF is at the core of competitive wholesale electricity markets and vertically integrated utility operations. The ACOPF simultaneously co-optimizes real and reactive power. First formulated over half a century ago in 1962 by Carpentier, the ACOPF is the most representative mathematical programming-based formulation of steady-state operations in AC networks. However the ACOPF is not solved in practice due to the nonconvex, nonlinear structure of the problem, for which finding the globally optimal solution is known to be NP-hard. Instead, least-cost unit commitment and generation dispatch in the day-ahead, intra-day, and real-time markets is determined with numerous simplifications of the ACOPF. This work presents a series of essays on the ACOPF problem, which include formulations, approximations, and applications in the electricity markets. The main themes center around ACOPF modeling fundamentals, followed by local and global solution methods to solve the problem for a variety of applications in the electricity markets. Original contributions of these essays include an alternative formulation of the ACOPF, a successive linear programming algorithm to solving the ACOPF for the real-time market, an outer approximation method to solving integrated ACOPF-unit commitment as a mixed-integer linear program for the day-ahead market, and applications of convex relaxations to the ACOPF and its approximations for the purpose of globally optimal storage integration. These contributions are concluded with a discussion of potential future directions of work.},
author = {Castillo, Anya},
file = {::},
keywords = {Alternating Current Optimal Power Flow (ACOPF),Global Solution Methods,Local Solution Methods,Optimal Power Flow (OPF),Outer Approximation Method (OA),Second-order Conic Programming (SOCP),Semidefinite Programming (SDP),Successive Linear Programming (SLP)},
pages = {266},
school = {The Johns Hopkins University},
title = {{Essays on the ACOPF problem: formulations, approximations, and applications in the electricity markets}},
type = {PhD Dissertation},
year = {2016}
}
