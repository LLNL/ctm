@article{Jabr2015,
abstract = {Renewable energy sources and energy storage systems present specific$\backslash$nchallenges to the traditional optimal power flow (OPF) paradigm.$\backslash$nFirst, storage devices require the OPF to model charge/discharge$\backslash$ndynamics and the supply of generated power at a later time. Second,$\backslash$nrenewable energy sources necessitate that the OPF solution accounts$\backslash$nfor the control of conventional power generators in response to errors$\backslash$nof renewable power forecast, which are significantly larger than$\backslash$nthe traditional load forecast errors. This paper presents a sparse$\backslash$nformulation and solution for the affinely adjustable robust counterpart$\backslash$n(AARC) of the multi-period OPF problem. The AARC aims at operating$\backslash$na storage portfolio via receding horizon control; it computes the$\backslash$noptimal base-point conventional generation and storage schedule for$\backslash$nthe forecasted load and renewable generation, together with the constrained$\backslash$nparticipation factors that dictate how conventional generation and$\backslash$nstorage will adjust to maintain feasible operation whenever the renewables$\backslash$ndeviate from their forecast. The approach is demonstrated on standard$\backslash$nIEEE networks dispatched over a 24-h horizon with interval forecasted$\backslash$nwind power, and the feasibility of operation under interval uncertainty$\backslash$nis validated via Monte Carlo analysis. The computational performance$\backslash$nof the proposed approach is compared with a conventional implementation$\backslash$nof the AARC that employs successive constraint enforcement.},
author = {Jabr, Rabih A and Karaki, Sami and Korbane, Joe Akl},
doi = {10.1109/TPWRS.2014.2365835},
file = {::},
journal = {IEEE Trans. Power Syst.},
keywords = {Discharges (electric),Energy storage,Generators,Op},
number = {5},
pages = {2790--2799},
title = {{Robust multi-period OPF with storage and renewables}},
volume = {30},
year = {2015}
}
