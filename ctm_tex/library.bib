doi = {10.1021/es202347q},
file = {::},
issn = {1520-5851},
journal = {Environmental science {\&} technology},
month = {feb},
pmid = {22201325},
title = {{Electric Vehicles in China: Emissions and Health Impacts.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22201325},
year = {2012}
}
