keywords = {Fault currents,load flow analysis,optimization methods,power generation planning},
number = {April},
pages = {1--7},
title = {{Optimal Power Flow as a Tool for Fault Level Constrained Network Capacity Analysis}},
url = {http://hdl.handle.net/1842/637},
year = {2004}
}
