        },
        "nonnegative_number": {
            "type": "number",
            "minimum" : 0
        },
        "positive_integer": {
            "type": "integer",
            "exclusiveMinimum" : 0
        },
        "positive_number": {
            "type": "number",
            "exclusiveMinimum" : 0
        },
        "uid": {
            "anyOf": [
                { "type": "integer", "minimum": 0 },
                { "type": "string" }
            ]
        },
        "time_series_reference" : {
            "type": "object",
            "required": [
                "uid",
                "scale_factor"
