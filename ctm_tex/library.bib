issn = {19493029},
journal = {IEEE Trans. Sustainable Energy},
keywords = {Flexibility,Wind energy,interval optimization,pumped hydro energy storage,stochastic optimization,unit commitment},
number = {2},
pages = {786--796},
title = {{Coupling pumped hydro energy storage with unit commitment}},
volume = {7},
year = {2016}
}
