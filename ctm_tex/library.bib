@article{Gemine2014a,
abstract = {In order to operate an electrical distribution network in a secure and cost-efficient way, it becomes necessary, due to the rise of renewable energy based distributed generation, to develop Active Network Management (ANM) strategies. These strategies rely on short-term policies that control the power injected by generators and/or taken off by loads so as to avoid congestions or voltage problems. While simple ANM strategies would curtail the production of generators, more advanced ones would move the consumption of loads to relevant time periods so as to maximize the potential of renewable energy sources. However, such advanced strategies imply solving large-scale optimal sequential decision-making problems under uncertainty, something which is complex. In order to promote the development of computational techniques for active network management, we detail a generic procedure for formulating ANM decision problems as Markov Decision Processes. We also specify it to a 75-bus distribution network. The resulting test instance is available at http://www.montefiore.ulg.ac.be/{\~{}}anm/. It can be used as testbed for comparing existing computational techniques as well as for developing new ones.},
archivePrefix = {arXiv},
