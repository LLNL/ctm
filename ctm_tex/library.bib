keywords = {2010,90c11,acknowledge yeesian ng and,acknowledgements the authors gratefully,convex relaxations,formulations,mathematics subject classification,mixed-integer programming,ondˇ},
title = {{Strong convex relaxations and mixed-integer programming formulations for trained neural networks}},
