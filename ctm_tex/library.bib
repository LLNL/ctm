issn = {19449925},
journal = {IEEE Power and Energy Society General Meeting},
keywords = {Fault currents,load flow,optimization,power system analysis computing,switchgear},
title = {{A network reconfiguration algorithm for the reduction of expected fault currents}},
year = {2013}
