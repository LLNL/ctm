{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "required": [
        "time_series_data"
    ],
    "properties": {
        "time_series_data": {
            "type": "object",
            "required": [
                "uid",
                "values"
            ],
            "properties": {
                "uid": {
                    "type": "array",
                    "items": {
                        "$ref": "#/$defs/uid"
                    }
                },
                "name": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "timestamp": {
                    "type": "array",
                    "items": {
                        "$ref": "#/$defs/nonnegative_integer"
                    }
                },
                "values": { }
            }
        }
    },
    "$defs": {
        "nonnegative_integer": {
            "type": "integer",
            "minimum" : 0
        },
        "uid": {
            "anyOf": [
                { "type": "integer", "minimum": 0 },
                { "type": "string" }
            ]
        }
    }
}
