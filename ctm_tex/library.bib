journal = {IEEE Trans. Smart Grid},
keywords = {Current-controlled inverters.,DC-AC power conversion,Distributed control,Inverters,Numerical stability,Overvoltage protection,Photovoltaic power systems,Power distribution control,Power quality,Power system stability,Reactive power,Reactive power control,Stability analysis,Stability criteria,Voltage control},
number = {c},
pages = {1--11},
title = {{Voltage stability in a grid-connected inverter with automatic Volt-Watt and Volt-VAR functions}},
volume = {3053},
year = {2017}
}
