journal = {IEEE Transactions on Power Systems},
keywords = {Batteries,Distributed generation (DG),Equations,Generators,Mathematical model,Microgrids,Power generation,distributed power generation,energy storage,load flow,microgrid,microgrid energy storage device,optimal control,optimal power flow,optimal power flow (OPF),power consumption,power generation control,power management improvement,renewable energy source,smart grid,smart power grids,storage device match energy generation},
